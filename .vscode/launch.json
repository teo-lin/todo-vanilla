{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "nodemon:app",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v20.14.0/bin/nodemon",
      "program": "${workspaceFolder}/src/app.js",
      "restart": true,
      "console": "internalConsole",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
